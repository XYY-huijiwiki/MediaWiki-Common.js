{
  "name": "@xyy-huijiwiki/special-file-display",
  "version": "0.1.6",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && babel dist/common.js --out-dir dist && terser dist/common.js --output dist/common.js",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "sync": "tsc sync/index.ts --target esnext --module nodenext --moduleResolution nodenext && node sync/index.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@types/jquery": "^3.5.29",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.10",
    "mustache": "^4.2.0",
    "prettier": "^3.2.5",
    "puppeteer": "^24.1.1",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "vite": "^5.2.7"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
